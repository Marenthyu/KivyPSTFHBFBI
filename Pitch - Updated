from kivy.uix.widget import Widget

class Game(App):
    def build(self):
        interface = BoxLayout(orientation='horizontal')
        interface.add_widget(Field)
        return interface

class Field(Widget):
            def GridDef(self): #Definiert das Gridlayout in Bezug auf Spalten, Reihen und Breite und Hoehe
                layout = GridLayout(cols=14, rows=16)
                GridLayout.height = s
                GridLayout.width = s
            def Sizing(self):
                Window.fullscreen = True
                h = Window.height
                w = Window.width

                if h < w:
                            s = h

                            Field.GridDef(self)
                else:
                            s = w

                            Field.GridDef(self)

            def Pitch(self):

                Field.Sizing(self)

                layout.add_widget(Image(source='Weiss.png'))
